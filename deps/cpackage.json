{
	"name": "PaccAppDependencies",
	"projects": [
		{
			"name": "fmtlib",
			"type": "interface",
			"includeFolders": {
				"interface": [ "fmt/include" ]
			},
			"defines": {
				"interface": "FMT_HEADER_ONLY"
			},
			"description": "FMT lib interface project"
		},
		{
			"name": "nlohmann_json",
			"type": "interface",
			"includeFolders": {
				"interface": [ "nlohmann_json/include" ]
			},
			"description": "JSON lib interface project"
		},
		{
			"name": "tiny-process-lib",
			"type": "interface",
			"includeFolders": {
				"interface": "tiny-process-lib/include"
			},
			"filters": {
				"configurations:Debug": {
					"linkerFolders": { "interface": "tiny-process-lib/lib/x64/Debug" }
				},
				"configurations:Release": {
					"linkerFolders": { "interface": "tiny-process-lib/lib/x64/Release" }
				}
			},
			"dependencies": {
				"interface": [ "tiny-process-library" ]
			},
			"description": "Process lib interface project"
		}
	],
	"version": "0.2.1",
	"description": "Workspace for Pacc app dependencies"
